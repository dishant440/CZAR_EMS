{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "functions": {
    "server/server.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/build/index.html"
    }
  ]
}